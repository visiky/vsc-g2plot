{
  "name": "vsc-g2plot",
  "displayName": "g2plot-vscode-extension",
  "description": "vscode extension for G2Plot",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.57.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "g2plot.assets.add",
        "when": "editorLangId =~ /^typescriptreact$|^javascriptreact$/",
        "title": "G2Plot: 资产添加到此处",
        "group": "G2Plot"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "g2plotAssetView",
          "name": "G2Plot 资产"
        }
      ]
    },
    "menus": {
      "editor/context": [
        {
          "command": "g2plot.assets.add",
          "when": "editorLangId =~ /^typescriptreact$|^javascriptreact$/",
          "group": "navigation@0"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "peerDependencies": {
    "vscode": "^1.1.37"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.57.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  }
}
